  object dstParcelas: TSQLDataSet
    CommandText = 
      'Select P.rec_nrvend, P.rec_cdgrup, P.rec_nrcota, P.rec_nrparc, P' +
      '.rec_cdbanc, P.rec_dtvenc, P.rec_dtpaga, P.rec_vlparc, P.REC_TOT' +
      'AL_PAGO, p.rec_nossonumero  from PARCELAS P Where (P.REC_NRVEND ' +
      '= :pVENDA) and (P.REC_CDBANC = :pBANCO) And (P.REC_DTVENC >= :pD' +
      'TVENC) AND (P.REC_DTPAGA IS Null) AND ((P.REC_TOTAL_PAGO = 0) or' +
      ' (P.REC_TOTAL_PAGO IS null));'
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftInteger
